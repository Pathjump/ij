                    <h2>You are now working at <a href="#">{%if userjob%}{{userjob.internship.company}}{%endif%}</a> as a <a href="#">{%if userjob%}{{userjob.internship.title}}{%endif%}</a></h2>

                    <div class="wrapper">

                        <div class="inboxBlock">

                            <div class="simpleTabsContent" style="display: block;">
                                <button type="submit"><img src="{{asset('img/eyeIcon.png')}}" width="19" height="12"></button>
                                <button type="submit"><img src="{{asset('img/taskList.png')}}" width="28" height="16"></button>

                              <div class="perPage">
                                    <span>Filter :</span>
                                    <select class="chzn-select" id="statusFilter">
                                        <option value=""></option>
                                        <option {%if status=="new"%}selected="selected"{%endif%}>new</option>
                                        <option {%if status=="inprogress"%}selected="selected"{%endif%}>inprogress</option>
                                        <option {%if status=="done"%}selected="selected"{%endif%}>done</option>
                                        <option {%if status=="all"%}selected="selected"{%endif%}>all</option>
                                    </select>
                                </div>


                                <div class="perPage" id="perpageFilter">
                                    <span>Per page :</span>
                                    <select  class="chzn-select" id="perpageFilter" tabindex="2" name="tasks_per_show_page_{{ user.id }}" onchange="setCookie('tasks_per_show_page_{{ user.id }}', this.value, 86391359);window.location.reload();">
                                        {% for perPageNumber in 10 .. 100 %}
                                            {% if perPageNumber is divisibleby(10) %}
                                                <option{% if tasksPerPage == perPageNumber %} selected="selected"{% endif %} value="{{ perPageNumber }}">{{ perPageNumber }}</option>
                                            {% endif %}
                                        {% endfor %}
                                    </select>
                                </div>

                                <div class="perPage">
                                    <span>Page :</span>
                                    <select class="chzn-select" id="pageFilter" tabindex="2">
                                        <option value=""></option>
                                        {% for pageNumber in 1 .. lastPageNumber %}
                                            <option{% if page == pageNumber %} selected="selected"{% endif %} value="{{ pageNumber }}">{{ pageNumber }}</option>
                                        {% endfor %}
                                    </select>
                                </div>

                                <table class="msgsTable">
                                    <thead>
                                        <td>&nbsp;</td>
                                        <td>Task Name</td>
                                        <td>Date</td>
                                        <td>Status</td>
                                        <td>Show Task</td>
                                    </thead>
                                    {% if entities %}
                                    {% for entity in entities %}
                                    <tr{% if entity.status == 'new' %} class="read"{% endif %}>
                                        <td>&nbsp;</td>
                                        <td><a href="{{ path('student_task_show', { 'id': entity.id }) }}">{{ entity.title }}</a></td>
                                        <td><a href="{{ path('student_task_show', { 'id': entity.id }) }}">{% if entity.createdAt %}{{ entity.createdAt|date('Y-m-d H:i:s') }}{% endif %}</a></td>
                                        <td class="case"><a href="{{ path('student_task_show', { 'id': entity.id }) }}"><span class="{% if entity.status == 'new' %}new{% elseif entity.status == 'inprogress' %}inprog{% else %}done{% endif %}"></span>{% if entity.status == 'new' %}New{% elseif entity.status == 'inprogress' %}Inprogress{% else %}Done{% endif %}</a></td>
                                        <td class="showTask"><a onclick="$('#Task_{{ entity.id }}_description').toggle();" href="javascript:void(0)">showTask</a></td>
                                    </tr>
                                    <tr style="display: none;" id="Task_{{ entity.id }}_description">
                                        <td colspan="5" class="taskDetails">
                                            {{ entity.description }}
                                        </td>
                                    </tr>
                                    {% endfor %}
                                     {% endif %}
                                </table>

                                {% if lastPageNumber > 1 %}
                                <ul class="pging">
                                    {% if page > 1 %}
                                    <li><a href="{{ url('student_task',{'loginName': user.loginName,'status': status}) }}">&laquo;&laquo;</a></li>
                                    <li><a href="{{ url('student_task',{'loginName':user.loginName,'status': status,'page': page - 1}) }}">&laquo;</a></li>
                                    {% endif %}

                                    {% for numberOfPage in 1..lastPageNumber %}
                                    {% if numberOfPage > page - 3 and numberOfPage < page + 3 %}
                                        {% if page == numberOfPage %}<li class="crnt"><a href="{{ url('student_task',{'loginName':user.loginName,'status': status ,'page': numberOfPage}) }}">{{ numberOfPage }}</a></li>{% else %}<li><a href="{{ url('student_task',{'loginName':user.loginName ,'status': status,'page': numberOfPage}) }}" >{{ numberOfPage }}</a></li>{%endif%}
                                    {% endif %}
                                    {% endfor %}

                                    {% if page < lastPageNumber %}
                                            <li><a href="{{ url('student_task',{'loginName':user.loginName,'status': status, 'page': page + 1}) }}" >&raquo;</a></li>
                                            <li><a href="{{ url('student_task',{'loginName':user.loginName,'status': status, 'page': lastPageNumber}) }}" >&raquo;&raquo;</a></li>
                                    {% endif %}
                                </ul>
                                {% endif %}
                            </div>
                        </div>
                    </div>

                </aside>
            </section>
                {%endif%}


            {% if user.cvs %}
            <section class="comntSection" id="crntHired">
                <aside class="copmanyInfo" id="myCvs">
                    <h1>My Resumes</h1>
                    <div class="CVs-box">
                        <ul>
                            {% for cv in user.cvs %}
                            <li>
                                <input type="checkbox" name="checkbox" class="cvcheckbox" value="{{ cv.id }}"/>
                                <a title="{{ cv }}" href="{{ path('user_portal_home',{'loginName' : user.loginName,'cvId' : cv.id}) }}">{{ cv.name }}</a>
                                <div class="onoffswitch">
                                    <input type="checkbox" name="onoffswitch" class="onoffswitch-checkbox isActiveCv" id="{{ cv.id }}" {% if cv.isActive %}checked="checked"{% endif %}>
                                    <label class="onoffswitch-label" for="{{cv.id}}">
                                        <div class="onoffswitch-inner">
                                            <div class="onoffswitch-active">ON</div>
                                            <div class="onoffswitch-inactive">OFF</div>
                                        </div>
                                        <div class="onoffswitch-switch"></div>
                                    </label>
                                </div>
                            </li>
                            {% endfor %}
                        </ul>
                    </div>
                    <br>
                    <a class="button addcvbutton" href="{{path('cv_new')}}" value="AddCV">Add Resume</a>
                    <button class="button del_btn" value="Delete">Delete</button>
                    <button class="button pub_btn" value="Publish">Publish</button>
                    <button class="button unpub_btn" value="UnPublish">Unpublish</button>
                    <div class="clear"></div>

                </aside>
            </section>
            {% endif %}

                {%if appliedJobs%}
            <section class="comntSection" id="crntHired">
                <aside class="copmanyInfo" id="myCvs">
                    <h1>Job Applications</h1>
                    <div class="CVs-box">
                        <ul>
                        {%for userjob in appliedJobs%}
                            <li>
                                <a title="" href="{{path('internship_show',{'id': userjob.internship.id})}}"> {{userjob.internship.title}}</a>

                            </li>
                            {%endfor%}
                        </ul>
                    </div>
                    <div class="clear"></div>
                </aside>
            </section>
                {%endif%}

        </section>
    </div>
            {%else%}
                         <section id="portalHome" class="comntSection">
                            <div class="alertConfirmation">Please Activate Your Account!</div>
                            <div style="padding:5px;margin:10px;width:320px;border:1px solid #CCC;">In order to protect our database we ask users to confirm their email. 
                            Please check your inbox for a email from us, if you have not received it 
                            check your spam folder. If you still can not find the email, please contact support@internjump.com</div>
                         </section>
            {%endif%}
</section>





{%endblock%}

