    <script>
    $(document).ready(function(){
        $('#logedInStudent,#userHome').addClass('crnt');
        $('#studentNav').show();

        $('.progressBar').hover(function() {
        if($('.moredetails').is(':hidden')){
        $('.moredetails').slideToggle(1000);
        $('.detailsLink').text('hide details');
        }
    });

        $('.detailsLink').click(function(){
            $('.moredetails').slideToggle(1000,function(){
                if($('.moredetails').is(':hidden')){
                $('.detailsLink').text('show details');
            }else{
                $('.detailsLink').text('hide details');
            }
            });
    });
    });

        // add_CV button
      /*  $('.addcvbutton').click(function(){
            window.location= '{{path('cv_new')}}';
        });
      */

        // Publish Button
        $('.pub_btn').click(function(){
            var count = 0;
            var status= 1;
            var method= "pubUnpub";
            $(".cvcheckbox:checked").each(function(){//input[type=checkbox][checked]
                count+= 1;
            });
            if(count==0)//in case no checked checkbox
            {
                jAlert('Please Choose CV(s) to be published','Confirm Publish');
            }
            else
            {
             jConfirm('ok','cancel','You are about to Publish '+count+' Of your CV(s) <br />Are you sure you want to ', 'Confirm Publish', function (r) {
            if (r == true) {
                var cvsId=[];
                $('.loading').show();
                $(".cvcheckbox:checked").each(function(){
                    cvsId.push($(this).val());
                });
                //end .each


                    $.ajax({
                        url: cvActionsUrl+'/'+cvsId+'/'+status+'/'+method,
                        success: function(msg) {
                         },
                        complete: function(msg) {
                            //hide loading div
                            $('.loading').hide();
                            //now reload the page
                            window.location.reload();
                        }
                    });



            }});

        }//end else

        });


        //Unpublish Button
        $('.unpub_btn').click(function(){
            var count = 0;
            var status= 0;
            var method= "pubUnpub";
            $(".cvcheckbox:checked").each(function(){//input[type=checkbox][checked]
                count+= 1;
            });
            if(count==0)//in case no checked checkbox
            {
                jAlert('Please Choose CV(s) to be Unpublished','Confirm Publish');
            }
            else
            {
             jConfirm('ok','cancel','You are about to UnPublish '+count+' Of your CV(s) <br />Are you sure you want to ', 'Confirm UnPublish', function (r) {
            if (r == true) {

                var cvsId=[];
                $('.loading').show();
                $(".cvcheckbox:checked").each(function(){
                    cvsId.push($(this).val());
                });
                //end .each

                    $.ajax({
                        url: cvActionsUrl+'/'+cvsId+'/'+status+'/'+method,
                        success: function(msg) {
                         },
                        complete: function(msg) {
                            //hide loading div
                            $('.loading').hide();
                            //now reload the page
                            window.location.reload();
                        }
                    });




            }});

        }//end else

        });


        //Delete Button
        $('.del_btn').click(function(){
            var count = 0;
            var status= 2;
            var method= "delete";
            $(".cvcheckbox:checked").each(function(){//input[type=checkbox][checked]
                count+= 1;
            });
            if(count==0)//in case no checked checkbox
            {
                jAlert('Please Choose CV(s) to be Deleted','Confirm Delete');
            }
            else
            {
             jConfirm('ok','cancel','You are about to Delete '+count+' Of your CV(s) <br />Are you sure you want to ', 'Confirm Delete', function (r) {
            if (r == true) {

                var cvsId=[];
                $('.loading').show();
                $(".cvcheckbox:checked").each(function(){
                    cvsId.push($(this).val());
                });
                //end .each

                    $.ajax({
                        url: cvActionsUrl+'/'+cvsId+'/'+status+'/'+method,
                        success: function(msg) {
                         },
                        complete: function(msg) {
                            //hide loading div
                            $('.loading').hide();
                            //now reload the page
                            window.location.reload();
                        }
                    });




            }});

        }//end else

        });

        $('#statusFilter').change(function(){
                 var status = $('#statusFilter option:selected').val();
                 window.location= portalPageActionsUrl+status;
        });

        $('#pageFilter').change(function(){
                var page=$('#pageFilter option:selected').val();
                var status ='{{status}}';
                 window.location= portalPageActionsUrl+status+"/"+page;
        });

        $('.quizMore').hover(function(){
            $('.quizMoreData').toggle(300);
        },function(){
            $('.quizMoreData').toggle(300);
        });
    </script>


