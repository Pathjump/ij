<!DOCTYPE html>
<html>
    <head>
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>{% block title %}Admin Home{% endblock %}</title>
        {% block stylesheets %}
        <link href="{{ asset('bundles/objectsadmin/css/bootstrap.min.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        <style type="text/css">
                body {padding-top: 40px;}
                button,input,select,textarea {margin: 5px;padding: 5px;}
        </style>
        <link rel="shortcut icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <div class="container-fluid">
            <div class="topbar-wrapper">
                <div data-dropdown="dropdown" class="topbar">
                    <div class="topbar-inner">
                        <div class="container-fluid">
                            <img src="{{ asset('/img/internjump-backend-logo.png') }}" alt="Internjump" style="float: left;padding-right: 10px;"/>
                            <h3><a href="{{ url('admin_home') }}">Admin Home</a></h3>
                            <ul class="nav">
                                <li><a href="{{ url('sonata_admin_dashboard') }}" style="padding: 10px 5px;">Dashboard</a></li>
                                <li><a href="{{ url('admin_change_constants') }}" style="padding: 10px 5px;">ChangeConstants</a></li>
                                <li><a href="{{ url('admin_change_api_constants') }}" style="padding: 10px 5px;">ChangeApiConstants</a></li>
                                <li><a href="{{ url('admin_edit_about_us') }}" style="padding: 10px 5px;">AboutUs</a></li>
                                <li><a href="{{ url('admin_edit_terms_of_Service') }}" style="padding: 10px 5px;">TermsOfService</a></li>
                                <li><a href="{{ url('admin_edit_privacy_policy') }}" style="padding: 10px 5px;">PrivacyPolicy</a></li>
                                <li><a href="{{ url('admin_edit_employers_data_page') }}" style="padding: 10px 5px;">EmployersDataPage</a></li>
                                <li><a href="{{ url('admin_edit_students_data_page') }}" style="padding: 10px 5px;">StudentsDataPage</a></li>
                                <li><a href="{{ url('admin_edit_schools_page') }}" style="padding: 10px 5px;">SchoolsPage</a></li>
                                <li><a href="{{ url('admin_edit_site_forms') }}" style="padding: 10px 5px;">SiteForms</a></li>
                                <li><a href="{{ url('admin_edit_CampusReps') }}" style="padding: 10px 5px;">CampusReps</a></li>
                                
                                <li><a href="{{ url('logout') }}" style="padding: 10px 5px;">Logout</a></li>
                            </ul>
                        </div>
                    </div><!-- /topbar-inner -->
                </div><!-- /topbar -->
            </div>
            <br/>
            {% if app.session.hasFlash('notice') %}
            <div class="alert-message fail">
                <p>{{ app.session.flash('notice') | raw }}</p>
            </div>
            {% endif %}
            {% if app.session.hasFlash('success') %}
            <div class="alert-message success">
                <p>{{ app.session.flash('success') | raw }}</p>
            </div>
            {% endif %}
            {% if app.session.hasFlash('error') %}
            <div class="alert-message error">
                <p>{{ app.session.flash('error') | raw }}</p>
            </div>
            {% endif %}
            {% block body %}{% endblock %}
        </div>
        {% block javascripts %}
            <script src="{{ asset('js/jquery-1.8.1.js') }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
